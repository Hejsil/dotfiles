account jhc@dismail.de
host smtp.dismail.de
port 465
from jhc@dismail.de
user jhc@dismail.de
passwordeval "pass jhc@dismail.de"
auth on
tls_starttls off
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/hejsil/.config/msmtp/msmtp.log

