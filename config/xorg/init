#!/bin/execlineb -P
importas config XDG_CONFIG_HOME
cd $config

foreground { auto-xrandr }
foreground { xset r rate 300 50 }
foreground { xset s off }
foreground { xset s off -dpms }
foreground { setxkbmap -layout dk }
foreground { xmodmap -e "keycode 135 = Super_L" }
foreground { xmodmap -e "keycode 66 = NoSymbol" }
foreground { xrdb -merge xorg/resources }

bspwm
